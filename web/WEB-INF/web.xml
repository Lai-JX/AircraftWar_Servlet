<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>HttpServerLogin</servlet-name>
        <servlet-class>com.hitsz.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HttpServerRegister</servlet-name>
        <servlet-class>com.hitsz.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BattleMode</servlet-name>
        <servlet-class>com.hitsz.servlet.BattleMode</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HttpServerLogin</servlet-name>
        <url-pattern>/LoginInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HttpServerRegister</servlet-name>
        <url-pattern>/RegisterMsg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BattleMode</servlet-name>
        <url-pattern>/BattleMode</url-pattern>
    </servlet-mapping>
</web-app>